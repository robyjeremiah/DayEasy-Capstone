{
  "expo": {
    "name": "DayEasy",
    "slug": "Day-Easy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/SplashTest.PNG",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],

    "android": {
      "package": "com.bycedric.helloworld",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },

    "ios": {
      "supportsTablet": true
    },

    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
